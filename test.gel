// #import("libc")

let arr [heap; int]

const i string = "Hi"

func c(from #string) -> [from.length+1; u8] {

    PushAllocator(temporary_allocator)

    match cstr = Allocate(from.length+1) {
        Ok       => CopyBytes(from.length, from.data, cstr)
        Err(err) => panic(err)
    }

    cstr.(from.length) = 0
}

func main(args [string]) -> Optional(error) {
    libc.Printf(c("%s"), args.0)
    return None
}
